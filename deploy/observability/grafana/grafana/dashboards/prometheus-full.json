{
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "10.0.0"
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": ""
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": ""
    },
    {
      "type": "panel",
      "id": "bargauge",
      "name": "Bar gauge",
      "version": ""
    },
    {
      "type": "panel",
      "id": "table",
      "name": "Table",
      "version": ""
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    }
  ],
  "id": null,
  "uid": "prom-full",
  "title": "Prometheus â€“ Full Overview",
  "tags": [
    "prometheus",
    "infrastructure",
    "llm-server"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "15s",
  "editable": true,
  "style": "dark",
  "graphTooltip": 1,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "15s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h"
    ]
  },
  "templating": {
    "list": [
      {
        "name": "job",
        "label": "Job",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "Prometheus"
        },
        "refresh": 1,
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "query": "label_values(up, job)",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "instance",
        "label": "Instance",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "Prometheus"
        },
        "refresh": 1,
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "query": "label_values(up{job=~\"$job\"}, instance)",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "type": "dashboard",
        "name": "Annotations & alerts",
        "enable": true,
        "hide": false,
        "iconColor": "rgba(255, 96, 96, 1)",
        "builtIn": 1,
        "datasource": "-- Grafana --"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Prometheus Up",
      "description": "Is Prometheus itself up?",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 0,
        "y": 0
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "horizontal"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "up{job=\"prometheus\"}",
          "interval": "",
          "legendFormat": "{{instance}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          },
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN"
                },
                "1": {
                  "text": "UP"
                }
              }
            }
          ]
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Scrape Targets Up",
      "description": "Number of targets currently up (all jobs)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 4,
        "y": 0
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "sum"
          ],
          "fields": "",
          "values": false
        },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "horizontal"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(up)",
          "interval": "",
          "legendFormat": "up"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "percentage",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 0
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "type": "bargauge",
      "title": "Target State by Job",
      "description": "How many targets are up/down per job",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 8,
        "y": 0
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "basic",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (job) (up)",
          "legendFormat": "{{job}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "green",
                "value": 2
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Scrape Success / Failure Rate",
      "description": "Rate of successful & failed scrapes across all jobs",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "fieldConfig": {
        "defaults": {
          "drawStyle": "line",
          "lineWidth": 2,
          "fillOpacity": 10
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (job) (rate(prometheus_target_scrapes_total{scrape_job=~\"$job\",result=\"success\"}[5m]))",
          "legendFormat": "success {{job}}"
        },
        {
          "refId": "B",
          "expr": "sum by (job) (rate(prometheus_target_scrapes_total{scrape_job=~\"$job\",result=\"failed\"}[5m]))",
          "legendFormat": "failed {{job}}"
        }
      ]
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Scrape Duration (95th percentile)",
      "description": "High percentile scrape durations by job",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "drawStyle": "line",
          "lineWidth": 2
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le, job) (rate(prometheus_target_scrape_pool_scrape_duration_seconds_bucket{job=~\"$job\"}[5m])))",
          "legendFormat": "p95 {{job}}"
        }
      ]
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Prometheus TSDB: Series & Chunks",
      "description": "Number of series and chunks in memory",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "fieldConfig": {
        "defaults": {
          "drawStyle": "line",
          "lineWidth": 2
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "prometheus_tsdb_head_series",
          "legendFormat": "series"
        },
        {
          "refId": "B",
          "expr": "prometheus_tsdb_head_chunks",
          "legendFormat": "chunks"
        }
      ]
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Prometheus TSDB WAL Fsync Duration (p99)",
      "description": "Write-ahead log fsync duration, 99th percentile",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 12
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "drawStyle": "line",
          "lineWidth": 2
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.99, sum by (le) (rate(prometheus_tsdb_wal_fsync_duration_seconds_bucket[5m])))",
          "legendFormat": "wal fsync p99"
        }
      ]
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Prometheus Process CPU Usage",
      "description": "CPU seconds used by Prometheus process (rate)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 20
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "drawStyle": "line",
          "lineWidth": 2
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(process_cpu_seconds_total[5m])",
          "legendFormat": "cpu"
        }
      ]
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "Prometheus Memory Usage",
      "description": "Resident memory used by Prometheus process",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 20
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "drawStyle": "line",
          "lineWidth": 2
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "process_resident_memory_bytes",
          "legendFormat": "rss"
        }
      ]
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "Prometheus Goroutines & FDs",
      "description": "Number of goroutines and open file descriptors",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 20
      },
      "fieldConfig": {
        "defaults": {
          "drawStyle": "line",
          "lineWidth": 2
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "go_goroutines",
          "legendFormat": "goroutines"
        },
        {
          "refId": "B",
          "expr": "process_open_fds",
          "legendFormat": "open_fds"
        }
      ]
    },
    {
      "id": 11,
      "type": "table",
      "title": "Targets by Job and Instance",
      "description": "Current state of all targets",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 28
      },
      "options": {
        "showHeader": true
      },
      "transformations": [
        {
          "id": "labelsToFields",
          "options": {}
        }
      ],
      "targets": [
        {
          "refId": "A",
          "expr": "up",
          "legendFormat": "{{job}} {{instance}}"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    }
  ]
}